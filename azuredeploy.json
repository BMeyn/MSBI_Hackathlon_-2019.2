{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "StorageAccName": {
            "type": "string",
            "defaultValue": "storageaccmsbi"
        },

        "CustomVisionTrainingName":{
            "type": "string",
            "defaultValue": "cvtraining"
        },
        
        "CustomVisionTrainPriceTier":{
            "type": "string",
            "defaultValue": "S0"
        },

        "CustomVisionPredictionName":{
            "type": "string",
            "defaultValue": "cvprediction"
        },
        "CustomVisionPredPriceTier":{
            "type": "string",
            "defaultValue": "S0"
        },
        "ComputerVisionName":{
            "type": "string",
            "defaultValue": "mlsvision"
        },
        "ComputerVisionPriceTier":{
            "type": "string",
            "defaultValue": "S1"
        },
        "msbiFunctionAppName":{
            "type": "string",
            "defaultValue": "msbifunctionapp"
        },
        "msbiFunctionAppPlanName":{
            "type": "string",
            "defaultValue": "functionappplan"
        }
    },
    "variables": {
        "resourceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('StorageAccName'))]",
        "apiVersion": "[providers('Microsoft.Storage', 'storageAccounts').apiVersions[0]]",
        "CustomVisionTrainingKey": "[resourceId('Microsoft.CognitiveServices/accounts', parameters('CustomVisionTrainingName')).apiVersions[0]]"
    },
    "resources": [
        {
            "name": "[parameters('StorageAccName')]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2015-06-15",
            "location": "[resourceGroup().location]",
            "tags": {
                "displayName": "[parameters('StorageAccName')]"
            },
            "properties": {
                "accountType": "Standard_LRS"
            }
        },
        {
            "type": "Microsoft.Storage/storageAccounts/blobServices",
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('StorageAccName'), '/default')]",
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('StorageAccName'))]"
            ],
            "properties": {
                "cors": {
                    "corsRules": [
                        {
                            "allowedOrigins": [
                                "https://mlworkspace.azure.ai"
                            ],
                            "allowedMethods": [
                                "GET"
                            ],
                            "maxAgeInSeconds": 1800,
                            "exposedHeaders": [
                                "*"
                            ],
                            "allowedHeaders": [
                                "*"
                            ]
                        }
                    ]
                },
                "deleteRetentionPolicy": {
                    "enabled": false
                }
            }
        },
        {
            "type": "Microsoft.Storage/storageAccounts/blobServices/containers",
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('StorageAccName'), '/default/dropzone')]",
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts/blobServices', parameters('StorageAccName'), 'default')]",
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('StorageAccName'))]"
            ],
            "properties": {
                "publicAccess": "None"
        }
        },
        {
            "type": "Microsoft.Storage/storageAccounts/blobServices/containers",
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('StorageAccName'), '/default/results')]",
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts/blobServices', parameters('StorageAccName'), 'default')]",
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('StorageAccName'))]"
            ],
            "properties": {
                "publicAccess": "None"
            }
        },

        {
            "apiVersion": "2016-02-01-preview",
            "name": "[parameters('CustomVisionTrainingName')]",
            "type": "Microsoft.CognitiveServices/accounts",
            "location": "[resourceGroup().location]",
            "tags": {
                "displayName": "[parameters('CustomVisionTrainingName')]"
            },
            "sku": {
              "name": "[parameters('CustomVisionTrainPriceTier')]"
            },
            "kind": "CustomVision.Training",
            "properties": {}
        },
        {
            "apiVersion": "2016-02-01-preview",
            "name": "[parameters('CustomVisionPredictionName')]",
            "type": "Microsoft.CognitiveServices/accounts",
            "location": "[resourceGroup().location]",
            "tags": {
                "displayName": "[parameters('CustomVisionPredictionName')]"
            },
            "sku": {
                "name": "[parameters('CustomVisionPredPriceTier')]"
            },
            "kind": "CustomVision.Prediction",
            "properties": {}
        },
        {
            "type": "Microsoft.CognitiveServices/accounts",
            "sku": {
                "name": "[parameters('ComputerVisionPriceTier')]"
            },
            "kind": "ComputerVision",
            "name": "[parameters('ComputerVisionName')]",
            "tags": {
                "displayName": "[parameters('ComputerVisionName')]"
            },
            "apiVersion": "2016-02-01-preview",
            "location": "[resourceGroup().location]",
            "properties": {},
            "dependsOn": []
    },
        {
            "type": "Microsoft.Web/serverfarms",
            "apiVersion": "2018-02-01",
            "name": "[parameters('msbiFunctionAppPlanName')]",
            "tags": {
                "displayName": "[parameters('msbiFunctionAppPlanName')]"
            },
            "location": "[resourceGroup().location]",
            "sku": {
            "name": "Y1",
            "tier": "Dynamic"
            },
            "properties": {
             "name": "[parameters('msbiFunctionAppPlanName')]",
            "computeMode": "Dynamic"
            }
    },
            {
            "type": "Microsoft.Web/sites",
            "apiVersion": "2016-08-01",
            "name": "[parameters('msbiFunctionAppName')]",
            "tags": {
                "displayName": "[parameters('msbiFunctionAppName')]"
            },
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', parameters('msbiFunctionAppPlanName'))]"
            ],
            "kind": "functionapp",
            "properties": {
                "enabled": true,
                "hostNameSslStates": [
                    {
                        "name": "msbifunctionapp.azurewebsites.net",
                        "sslState": "Disabled",
                        "hostType": "Standard"
                    },
                    {
                        "name": "msbifunctionapp.scm.azurewebsites.net",
                        "sslState": "Disabled",
                        "hostType": "Repository"
                    }
                ],
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('msbiFunctionAppPlanName'))]",
                "reserved": false,
                "scmSiteAlsoStopped": false,
                "clientAffinityEnabled": false,
                "clientCertEnabled": false,
                "hostNamesDisabled": false,
                "containerSize": 1536,
                "dailyMemoryTimeQuota": 0,
                "httpsOnly": false
            }
        },
        {
            "type": "Microsoft.Web/sites/config",
            "apiVersion": "2016-08-01",
            "name": "[concat(parameters('msbiFunctionAppName'), '/web')]",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', parameters('msbiFunctionAppName'))]"
            ],
            "properties": {
                "numberOfWorkers": 1,
                "defaultDocuments": [
                    "Default.htm",
                    "Default.html",
                    "Default.asp",
                    "index.htm",
                    "index.html",
                    "iisstart.htm",
                    "default.aspx",
                    "index.php"
                ],
                "netFrameworkVersion": "v4.0",
                "phpVersion": "5.6",
                "pythonVersion": "",
                "nodeVersion": "",
                "linuxFxVersion": "",
                "requestTracingEnabled": false,
                "remoteDebuggingEnabled": false,
                "httpLoggingEnabled": false,
                "logsDirectorySizeLimit": 35,
                "detailedErrorLoggingEnabled": false,
                "publishingUsername": "$msbiFunctionApp",
                "scmType": "None",
                "use32BitWorkerProcess": true,
                "webSocketsEnabled": false,
                "alwaysOn": false,
                "appCommandLine": "",
                "managedPipelineMode": "Integrated",
                "virtualApplications": [
                    {
                        "virtualPath": "/",
                        "physicalPath": "site\\wwwroot",
                        "preloadEnabled": false,
                        "virtualDirectories": null
                    }
                ],
                "winAuthAdminState": 0,
                "winAuthTenantState": 0,
                "customAppPoolIdentityAdminState": false,
                "customAppPoolIdentityTenantState": false,
                "loadBalancing": "LeastRequests",
                "routingRules": [],
                "experiments": {
                    "rampUpRules": []
                },
                "autoHealEnabled": false,
                "vnetName": "",
                "siteAuthEnabled": false,
                "siteAuthSettings": {
                    "enabled": null,
                    "unauthenticatedClientAction": null,
                    "tokenStoreEnabled": null,
                    "allowedExternalRedirectUrls": null,
                    "defaultProvider": null,
                    "clientId": null,
                    "clientSecret": null,
                    "clientSecretCertificateThumbprint": null,
                    "issuer": null,
                    "allowedAudiences": null,
                    "additionalLoginParams": null,
                    "isAadAutoProvisioned": false,
                    "googleClientId": null,
                    "googleClientSecret": null,
                    "googleOAuthScopes": null,
                    "facebookAppId": null,
                    "facebookAppSecret": null,
                    "facebookOAuthScopes": null,
                    "twitterConsumerKey": null,
                    "twitterConsumerSecret": null,
                    "microsoftAccountClientId": null,
                    "microsoftAccountClientSecret": null,
                    "microsoftAccountOAuthScopes": null
                },
                "cors": {
                    "allowedOrigins": [
                        "https://functions.azure.com",
                        "https://functions-staging.azure.com",
                        "https://functions-next.azure.com"
                    ],
                    "supportCredentials": false
                },
                "localMySqlEnabled": false,
                "http20Enabled": false,
                "minTlsVersion": "1.2",
                "ftpsState": "AllAllowed",
                "reservedInstanceCount": 0,
                "fileChangeAuditEnabled": false
            }
        },
        {
            "type": "Microsoft.Web/sites/hostNameBindings",
            "apiVersion": "2016-08-01",
            "name": "[concat(parameters('msbiFunctionAppName'), '/', parameters('msbiFunctionAppName'), '.azurewebsites.net')]",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', parameters('msbiFunctionAppName'))]"
            ],
            "properties": {
                "siteName": "msbiFunctionApp",
                "hostNameType": "Verified"
            }
        },
        {
            "type": "microsoft.insights/components",
            "apiVersion": "2015-05-01",
            "name": "[parameters('msbiFunctionAppName')]",
            "location": "[resourceGroup().location]",
            "kind": "web",
            "properties": {
                "Application_Type": "web",
                "Request_Source": "IbizaWebAppExtensionCreate"
            }
        }
    ],
    "outputs": {
        "storageAccountKey": {
            "type": "string",
            "value": "[listKeys(variables('resourceId'), variables('apiVersion')).keys[0].value]"
            },
        "storageAccountEndpoint": {
                "type": "string",
                "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('StorageAccName'), ';AccountKey=', listKeys(variables('resourceId'), variables('apiVersion')).keys[0].value, ';EndpointSuffix=core.windows.net')]"
            },
        "CustomVisionTrainingKey": {
            "type": "string",
            "value": "[listKeys(variables('CustomVisionTrainingKey'), variables('apiVersion')).keys[0].value]"
            }
    }
}

